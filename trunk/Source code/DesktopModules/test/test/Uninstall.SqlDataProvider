GO

DROP INDEX {databaseOwner}[{objectQualifier}YourCompany_test].[IX_{objectQualifier}YourCompany_test]
GO

DROP TABLE {databaseOwner}[{objectQualifier}YourCompany_test]
GO

/** Drop Stored Procedures **/

DROP PROCEDURE {databaseOwner}[{objectQualifier}YourCompany_Gettests]
GO

