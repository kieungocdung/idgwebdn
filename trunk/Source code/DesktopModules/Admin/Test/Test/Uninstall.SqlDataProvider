/** Drop FAQs Table **/

ALTER TABLE {databaseOwner}[{objectQualifier}YourCompany_Test] DROP CONSTRAINT [FK_{objectQualifier}YourCompany_Test_{objectQualifier}Modules]
GO

ALTER TABLE {databaseOwner}[{objectQualifier}YourCompany_Test] DROP CONSTRAINT [PK_{objectQualifier}YourCompany_Test]
GO

DROP INDEX {databaseOwner}[{objectQualifier}YourCompany_Test].[IX_{objectQualifier}YourCompany_Test]
GO

DROP TABLE {databaseOwner}[{objectQualifier}YourCompany_Test]
GO

/** Drop Stored Procedures **/

DROP PROCEDURE {databaseOwner}[{objectQualifier}YourCompany_GetTests]
GO

